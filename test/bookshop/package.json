{
  "dependencies": {
    "@cap-js/telemetry": "*",
    "@cap-js/sqlite": "*",
    "@opentelemetry/host-metrics": "*",
    "@sap/cds-mtxs": "*"
  },
  "cds": {
    "_log": {
      "levels": {
        "cds": "error"
      }
    },
    "requires": {
      "telemetry": {
        "_kind": "to-jaeger",
        "__kind": "to-dynatrace",
        "___kind": "to-cloud-logging",
        "instrumentations": {
          "http": {
            "config": {
              "ignoreIncomingPaths": [
                "/odata/v4/admin/Authors"
              ]
            }
          }
        },
        "tracing": {
          "__hrtime": false,
          "_exporter": {
            "module": "@opentelemetry/sdk-trace-base",
            "class": "ConsoleSpanExporter"
          }
        },
        "_metrics": {
          "exporter": {
            "module": "@opentelemetry/sdk-metrics",
            "class": "ConsoleMetricExporter"
          }
        }
      },
      "[multitenancy]": {
        "multitenancy": true,
        "auth": {
          "users": {
            "user_tenant_1": {
              "tenant": "tenant_1",
              "roles": [
                "admin",
                "cds.Subscriber"
              ]
            },
            "user_tenant_2": {
              "tenant": "tenant_2",
              "roles": [
                "admin"
              ]
            }
          }
        }
      }
    }
  }
}
