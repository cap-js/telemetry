{
  "dependencies": {
    "@cap-js/telemetry": "*",
    "@cap-js/sqlite": "*",
    "@opentelemetry/host-metrics": "*",
    "@sap/cds-mtxs": "*"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      },
      "telemetry": {
        "_kind": "to-jaeger",
        "__kind": "to-dynatrace",
        "___kind": "to-cloud-logging",
        "instrumentations": {
          "http": {
            "config": {
              "ignoreIncomingPaths": [
                "/odata/v4/admin/Authors"
              ]
            }
          }
        }
      },
      "auth": {
        "kind": "mocked-auth",
        "users": {
          "user_tenant_1": {
            "tenant": "tenant_1",
            "roles": [
              "admin"
            ]
          },
          "user_tenant_2": {
            "tenant": "tenant_2",
            "roles": [
              "admin"
            ]
          }
        }
      },
      "multitenancy": true,
      "toggles": true
    }
  }
}
